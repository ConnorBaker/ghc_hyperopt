    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--project-path=./FibHaskell",
                "--component-name=bench:bench-fib",
                "--artifact-dir=./artifacts",
                "--tune-ghc",
                "--tune-ghc-all"
